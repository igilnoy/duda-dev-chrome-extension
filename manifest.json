{
  "name": "Duda Dev Info",
  "description": "Get additional information of Duda websites to ease development",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "permissions": ["activeTab", "<all_urls>"]
}